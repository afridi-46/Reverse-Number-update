      "source": [
        "n = int(input(\"Enter any number : \"))\n",
        "rev = 0\n",
        "\n",
        "while(n != 0):\n",
        "    reminder = n % 10\n",
        "    rev = rev * 10 + reminder\n",
        "    n = n // 10\n",
        "\n",
        "print(\"Reversed number : \", rev)"
      ],
